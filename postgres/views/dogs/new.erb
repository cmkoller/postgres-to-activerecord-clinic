<h1>New Dog</h1>

<form action="/dogs" method="post">
  <label for="name">Name:</label>
  <input type="text" name="name">

  <label for="owner">Owner:</label>
  <select name="owner">
    <% owners.each do |owner| %>
      <option value="<%= owner["id"] %>"><%= owner["name"] %></option>
    <% end %>
  </select>

  <input type="submit">
</form>
